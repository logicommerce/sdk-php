{
	"name" : "logicommerce/sdk",
	"require" : {
		"php": "^8.2",
		"monolog/monolog": "^3.3.1",
		"predis/predis": "^2.0.3"
	},
	"homepage" : "https://www.logicommerce.com",
	"minimum-stability" : "stable",
	"type" : "project",
	"description" : "PHP SDK for LogiCommerce",
	"keywords" : [
		"ecommerce",
		"sdk"
	],
	"autoload" : {
		"psr-4" : {
			"SDK\\" : "src/"
		},
		"exclude-from-classmap": ["/Tests/", "/test/", "/tests/"],
		"files" : [
			"src/_config.php"
		]
	},
	"autoload-dev": {
        "psr-4": {
        	"Test\\" : "test/"
        }
    },
	"require-dev" : {
		"phpunit/phpunit" : "^9.5",
		"phpunit/php-invoker" : "^3.1",
        "phpunit/php-code-coverage": "^9.2.3"
	}
}
